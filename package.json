    "@nestjs/schedule": "^6.0.1",
    "@nestjsplus/config": "^1.0.14",
    "@types/cron": "^2.4.3",
    "@types/lodash": "^4.17.20",
    "@types/passport-jwt": "^4.0.1",
    "@types/uuid": "^3.4.7",
    "axios": "^1.13.1",
    "class-validator": "^0.14.2",
